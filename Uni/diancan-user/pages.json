{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/saoma/saoma",
      "style": {
        "navigationBarTitleText": "扫码点餐",
        "navigationBarBackgroundColor": "#fefefe"
      }
    },
    {
      "path": "pages/home-page/page",
      "style": {
        "navigationBarTitleText": "Java1234餐饮店",
        "navigationBarBackgroundColor": "#f7d45f"
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "选择人数",
        "navigationBarBackgroundColor": "#fefefe",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/order-details/details",
      "style": {
        "navigationBarTitleText": "订单详情",
        "navigationBarBackgroundColor": "#f7d45f"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}